{
  "name": "ybasickit",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "start": "run-p watch:pug watch:scss2cssprefix watch:ts dev",
    "build": "run-p compile:pug compile:scss2cssprefix compile:ts prod",
    "fix": "yarn -s format && yarn -s lint:fix",
    "format": "prettier --write --loglevel=warn '{public,src}/**/*.{js,jsx,ts,tsx,html,gql,graphql,json}'",
    "lint": "yarn -s lint:style; yarn -s lint:es",
    "lint:fix": "yarn -s lint:style:fix && yarn -s lint:es:fix",
    "lint:es": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:es:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:conflict": "eslint-config-prettier 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:style": "stylelint 'src/**/*.{css,less,sass,scss}'",
    "lint:style:fix": "stylelint --fix 'src/**/*.{css,less,sass,scss}'",
    "compile:pug": "pug src/pug/ --hierarchy -o dist/ -P",
    "watch:pug": "pug src/pug/ --hierarchy -o dist/ -w -P",
    "compile:css2cssprefix": "postcss dist/assets/css/style.css -u autoprefixer -o dist/assets/css/style.css",
    "compile:scss2css": "sass src/scss/style.scss:dist/assets/css/style.css --style=expanded --source-map-urls=relative",
    "compile:scss2cssprefix": "run-s compile:scss2css compile:css2cssprefix",
    "watch:scss2cssprefix": "chokidar \"src/scss/\" --command \"yarn compile:scss2cssprefix\" --initial",
    "compile:ts": "webpack --config webpack.prod.js",
    "watch:ts": "webpack --config webpack.dev.js --watch",
    "clean": "rimraf ./dist",
    "dev": "node tasks/index.js develop",
    "prod": "node tasks/index.js production",
    "min:css": "cleancss -o dist/css/style.css dist/css/style.css",
    "delete:map": "rimraf dist/**/*.map",
    "prepare:all": "run-p min:js min:css delete:map"
  },
  "keywords": [],
  "author": "Toru Yaginuma",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@types/babel__core": "^7.1.14",
    "@types/babel__preset-env": "^7.9.1",
    "@types/browser-sync": "^3001.0.28-alpha",
    "@types/core-js": "^2.5.4",
    "@types/eslint": "^7.2.13",
    "@types/fibers": "^3.1.0",
    "@types/mini-css-extract-plugin": "^1.4.3",
    "@types/prettier": "^2.3.0",
    "@types/pug": "^2.0.4",
    "@types/sass": "^1.16.0",
    "@types/stylelint": "^13.13.0",
    "@types/webpack-dev-server": "^3.11.4",
    "@types/youtube": "^0.0.44",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "autoprefixer": "^10.3.1",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.27.4",
    "chokidar-cli": "^2.1.0",
    "clean-css-cli": "^5.3.0",
    "css-loader": "^5.2.6",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "fibers": "^5.0.0",
    "lint-staged": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.2",
    "postcss-cli": "^8.3.1",
    "postcss-loader": "^6.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.1",
    "pug": "^3.0.2",
    "pug-cli": "github:pugjs/pug-cli#master",
    "sass": "^1.34.0",
    "simple-git-hooks": "^2.4.1",
    "stylelint": "^13.13.1",
    "stylelint-config-recess-order": "^2.4.0",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0",
    "ts-loader": "^9.2.2",
    "typescript": "^4.3.2",
    "typesync": "^0.8.0",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@types/three": "^0.130.1",
    "core-js": "^3.15.2",
    "gsap": "^3.6.1",
    "intersection-observer": "^0.12.0",
    "three": "^0.131.1"
  }
}
